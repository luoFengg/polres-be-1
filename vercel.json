{
  "version": 2,
  "name": "koperasi-backend-api",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL_FORCE_NO_BUILD_CACHE": "1"
  },
  "buildCommand": "npm run build"
}
